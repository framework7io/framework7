.navbar
  .navbar-inner
    .left.sliding
      a(href="swiper.html").back.link
        i.icon.icon-back
        span Back
    .center.sliding 3D Flip
    .right
      a(href="#").open-panel.link.icon-only
        i.icon.icon-bars
.pages.navbar-through
  .page(data-page="swiper-3d-flip")
    .page-content
      .swiper-container.swiper-init.ks-demo-slider.ks-cube-slider(data-effect="flip")
        .swiper-wrapper
          - for (var i = 1; i < 10; i++)
            .swiper-slide(style="background-image:url(http://lorempixel.com/800/800/people/#{i})") Slide #{i}